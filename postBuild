conda install nodejs
npm install express
npm install cors
npm install n8n -g
export N8N_BASIC_AUTH_ACTIVE=true
export N8N_BASIC_AUTH_USER=admin
export N8N_BASIC_AUTH_PASSWORD=1234
export N8N_SECURE_COOKIE=false
# enable nbserverproxy
jupyter server extension enable --sys-prefix nbserverproxy
# streamlit launches at startup
mv node_call.py ${NB_PYTHON_PREFIX}/lib/python3.10/site-packages/
# enable streamlit extension
jupyter server extension enable --sys-prefix node_call
